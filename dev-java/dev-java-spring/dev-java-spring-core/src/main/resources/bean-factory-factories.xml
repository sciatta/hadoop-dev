<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- FactoryBean => singleton -->
    <bean id="singleton" class="com.sciatta.dev.java.spring.core.ioc.beanfactory.factories.UserSingletonFactoryBean"/>

    <!-- FactoryBean => prototype -->
    <bean id="prototype" class="com.sciatta.dev.java.spring.core.ioc.beanfactory.factories.UserPrototypeFactoryBean"/>

    <!-- ObjectFactory -->
    <bean id="objectFactory" class="com.sciatta.dev.java.spring.core.ioc.beanfactory.factories.UserObjectFactoryBean"/>
</beans>